{
	"figure": {
		"figsize": [16, 9],
		"title": "Diagrama psicrométrico",
		"x_label": "Temperatura (ºC)",
		"y_label": "Humedad específica (g_w / kg_da)"
	},
	"limits": {
		"range_temp_c": [0, 50],
		"range_humidity_g_kg": [0, 40],
		"altitude_m": 0,
		"step_temp": 1.0
	},
	"default": {"color": "darkgrey", "linewidth": 1, "linestyle": "-"},

	"saturation": {"color": [0.859, 0.0, 0.6], "linewidth": 4, "linestyle": "-"},
	"constant_rh": {"color": [0.0, 0.498, 1.0], "linewidth": 1, "linestyle": "-."},
	"constant_v": {"color": "lightgreen", "linewidth": 1, "linestyle": "--"},
	"constant_h": {"color": "orange", "linewidth": 1, "linestyle": "--"},
	"constant_wet_temp": {"color": "violet", "linewidth": 1, "linestyle": "-."},
	"constant_dry_temp": {"color": [0.855, 0.145, 0.114], "linewidth": 0.75, "linestyle": ":"},
	"constant_humidity": {"color": [0.0, 0.125, 0.376], "linewidth": 0.75, "linestyle": ":"},

	"chart_params": {
		"temp_increment": 1,
		"temp_range": [15, 40],
		"humidity_range": [0, 0.05],
		
		"with_constant_rh": true,
		"constant_rh_curves": [10, 20, 30, 40, 45, 50, 55, 60, 70, 80, 90],
		
		"with_constant_v": true,
		"constant_v_step": 0.02,
		"range_vol_m3_kg": [0.78, 1.0],

		"with_constant_h": true,
		"constant_h_step": 5,
		"range_h": [0, 250],

		"with_constant_wet_temp": true,
		"constant_wet_temp_step": 5,
		"range_wet_temp": [-10, 35],

		"with_constant_dry_temp": true,
		"constant_temp_step": 1,

		"with_constant_humidity": true,
		"constant_humid_step": 1
	}
}